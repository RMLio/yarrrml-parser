:target_000 a rmlt:LogicalTarget, rmlt:EventStreamTarget;
  rdfs:label "target-ldes";
  rmlt:serialization formats:N-Quads;
  rmlt:target :void_000;
  rmlt:ldesBaseIRI ex:ldes;
  rmlt:ldes :eventStream_000.
:void_000 a void:Dataset;
  void:dataDump <file://out-ldes.nq>.
:eventStream_000 ldes:timestampPath dc:created;
  ldes:versionOfPath dc:isVersionOf.
:map_sensor-reading-create_000 a rr:TriplesMap;
  rdfs:label "sensor-reading-create";
  rml:logicalSource _:b1_b57;
  rr:subjectMap [
    a rr:SubjectMap, fnml:FunctionTermMap;
    rr:termType rr:BlankNode;
    fnml:functionValue [
      rml:logicalSource _:b1_b57;
      rr:predicateObjectMap [
        rr:predicateMap [ rr:constant fno:executes ];
        rr:objectMap [
          rr:termType rr:IRI;
          rr:constant "https://w3id.org/imec/idlab/function#explicitCreate"
        ]
      ], [
        a rr:PredicateObjectMap;
        rr:predicateMap [
          a rr:PredicateMap;
          rr:constant <https://w3id.org/imec/idlab/function#iri>
        ];
        rr:objectMap [
          a rr:ObjectMap;
          rr:termType rr:IRI;
          rr:template "http://example.com/sensor/{sensor}"
        ]
      ]
    ];
    rr:graphMap :gm_000;
    rml:logicalTarget :target_000
  ];
  rr:predicateObjectMap [
    a rr:PredicateObjectMap;
    rr:predicateMap [
      a rr:PredicateMap;
      rr:constant ex:pressure
    ];
    rr:objectMap [
      a rr:ObjectMap;
      rr:termType rr:Literal;
      rml:reference "pressure"
    ]
  ], [
    a rr:PredicateObjectMap;
    rr:predicateMap [
      a rr:PredicateMap;
      rr:constant ex:temperature
    ];
    rr:objectMap [
      a rr:ObjectMap;
      rr:termType rr:Literal;
      rml:reference "temperature"
    ]
  ].
:gm_000 a rr:GraphMap;
  rr:constant "http://example.com/create".
