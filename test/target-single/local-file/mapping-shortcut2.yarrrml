sources:
  people-source:
    access: "/data/people.json"
    referenceFormulation: jsonpath
    iterator: "$.[*]"
targets:
  subject-target: ["/data/file.jsonld~dcat", "jsonld" ]
mappings:
  person:
    sources: people-source
    subjects:
      value: "http://example.org/$(id)"
      targets: subject-target
    predicateobjects:
      - predicates: foaf:name
        objects:
          value: "$(name)"
      - predicates: foaf:age
        objects:
          value: "$(age)"
