@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix : <http://mapping.example.com/> .
@prefix rmle: <https://w3id.org/imec/rml/ns/extensions#> .
@prefix rmlt: <http://semweb.mmlab.be/ns/rml-target#> .
@prefix formats: <http://www.w3.org/ns/formats/> .
@prefix htv: <http://www.w3.org/2011/http#> .

<#source> rdf:type rml:LogicalSource ;
	rdfs:label "ls_persons" ;
	rml:source "data.csv" ;
	rml:referenceFormulation ql:CSV .

# A triples map generating RDF Triples that are exported to the dynamically generated logical targets
<#TriplesMap> a rr:TriplesMap ;
  rml:logicalSource <#source> ;
  rr:subjectMap [
    a rr:subjectMap ;
    rr:template "http://example.org/{number}" ;
    rmle:LogicalTargetMap [
      a rmle:LogicalTargetMap ;
      rr:template "lt_person_{goesto}" ;
    ] ;
  ];
  rml:predicateObjectMap [
    a rml:PredicateObjectMap ;
    rml:predicate foaf:age ;
    rml:objectMap [
      a rml:ObjectMap ;
      rml:reference "number" ;
    ] ;
  ] ;
.

# A triples map generating logical targets
<#tm_lt_person> a rr:TriplesMap ;
  rml:logicalSource <#source> ;
  rr:subjectMap [
    a rr:subjectMap ;
    rr:template "lt_person_{goesto}" ;
    rr:class rml:LogicalTarget ;
    rml:logicalTarget rmle:ThisMapping ; # self-referencing logical target, making this a dynamic logical target
  ] ;
  rr:predicateObjectMap [
    a rr:PredicateObjectMap ;
    rr:predicate rml:target ;
    rr:objectMap [
      a rr:ObjectMap ;
      rr:template "t_person_{goesto}" ;
    ] ;
  ] ;
  rr:predicateObjectMap [
    a rr:PredicateObjectMap ;
    rr:predicate rmlt:serialization ;
    rr:objectMap [
      a rr:ObjectMap ;
      rr:constant formats:Turtle ;
    ] ;
  ] ;
.

# A triples map generating targets
<#tm_t_person> a rr:TriplesMap ;
  rml:logicalSource <#source> ;
  rr:subjectMap [
    rr:template "t_person_{goesto}" ;
    rml:class rml:Target;
    rml:logicalTarget rmle:ThisMapping ; # self-referencing logical target
  ] ;
  rml:predicateObjectMap [
    a rml:PredicateObjectMap ;
    rml:predicate void:dataDump ;
    rml:objectMap [
      a rml:ObjectMap ;
      rr:template "file:///{goesto}.ttl" ;
    ] ;
  ] ;
  rml:predicateObjectMap [
    a rml:PredicateObjectMap ;
    rml:predicate rdf:type ;
    rr:objectMap [
      a rr:ObjectMap ;
      rr:constant void:Dataset ;
    ] ;
  ] ;
.
