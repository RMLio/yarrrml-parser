@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix : <http://mapping.example.com/> .
@prefix ex: <http://example.com/> .

:map_employee_000 rml:logicalSource :source_000 ;
	rdf:type rr:TriplesMap ;
	rdfs:label "employee" ;
	rr:subjectMap :s_000 ;
	rr:predicateObjectMap :pom_000 .

:source_000 rdf:type rml:LogicalSource ;
	rml:source "data-1.csv" ;
	rml:referenceFormulation ql:CSV .

:s_000 rdf:type rr:SubjectMap ;
	rr:template "http://example.com/{id}" .

:pom_000 rdf:type rr:PredicateObjectMap ;
	rr:predicateMap :pm_000 ;
	rr:objectMap :om_000, :om_001 .

:pm_000 rdf:type rr:PredicateMap ;
	rr:constant ex:project .

:map_employee_001 rml:logicalSource :source_001 ;
	rdf:type rr:TriplesMap ;
	rdfs:label "employee" ;
	rr:subjectMap :s_001 ;
	rr:predicateObjectMap :pom_001 .

:source_001 rdf:type rml:LogicalSource ;
	rml:source "data-2.csv" ;
	rml:referenceFormulation ql:CSV .

:s_001 rdf:type rr:SubjectMap ;
	rr:template "http://example.com/{id}" .

:pom_001 rdf:type rr:PredicateObjectMap ;
	rr:predicateMap :pm_001 ;
	rr:objectMap :om_002, :om_003 .

:pm_001 rdf:type rr:PredicateMap ;
	rr:constant ex:project .

:map_project_000 rml:logicalSource :source_002 ;
	rdf:type rr:TriplesMap ;
	rdfs:label "project" ;
	rr:subjectMap :s_002 .

:source_002 rdf:type rml:LogicalSource ;
	rml:source "data-1.csv" ;
	rml:referenceFormulation ql:CSV .

:s_002 rdf:type rr:SubjectMap ;
	rr:template "http://example.com/{slug}" .

:map_project_001 rml:logicalSource :source_003 ;
	rdf:type rr:TriplesMap ;
	rdfs:label "project" ;
	rr:subjectMap :s_003 .

:source_003 rdf:type rml:LogicalSource ;
	rml:source "data-2.csv" ;
	rml:referenceFormulation ql:CSV .

:s_003 rdf:type rr:SubjectMap ;
	rr:template "http://example.com/{slug}" .

:om_000 rdf:type rr:ObjectMap ;
	rr:parentTriplesMap :map_project_000 ;
	rr:joinCondition :jc_000 .

:jc_000 rr:child "project-id" ;
	rr:parent "id" .

:om_001 rdf:type rr:ObjectMap ;
	rr:parentTriplesMap :map_project_001 ;
	rr:joinCondition :jc_001 .

:jc_001 rr:child "project-id" ;
	rr:parent "id" .

:om_002 rdf:type rr:ObjectMap ;
	rr:parentTriplesMap :map_project_000 ;
	rr:joinCondition :jc_002 .

:jc_002 rr:child "project-id" ;
	rr:parent "id" .

:om_003 rdf:type rr:ObjectMap ;
	rr:parentTriplesMap :map_project_001 ;
	rr:joinCondition :jc_003 .

:jc_003 rr:child "project-id" ;
	rr:parent "id" .

