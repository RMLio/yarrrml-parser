sources:
  people-source:
    access: "http://example.org/file.xml"
    contentType: "application/xml"
    operationType: read
    type: wot
    referenceFormulation: xpath
    iterator: "/"
    security: none
mappings:
  person:
    sources: people-source
    subjects:
      value: "http://example.org/$(id)"
    predicateobjects:
      - predicates: foaf:name
        objects:
          value: "$(name)"
      - predicates: foaf:age
        objects:
          value: "$(age)"
