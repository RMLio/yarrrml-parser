mappings:
  person:
    sources:
      - [test.csv~csv]
    s: http://example.com/person/$(id)
    po:
      - p: schema:creator
        o:
          mapping: project
          condition:
            function: equal
            parameters:
              - [str1, $(id)_$(name), s]
              - [str2, $(id)_$(name), o]
  project:
    sources:
      - [test.csv~csv]
    s: http://example.com/project/$(id)
    po:
      - [a, schema:Project]
