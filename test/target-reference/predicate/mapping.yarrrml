sources:
  people-source:
    access: "/data/people.json"
    referenceFormulation: jsonpath
    iterator: "$.[*]"
targets:
  predicate-target: ["/data/file.ttl.zip~dcat", "turtle", "zip"]
mappings:
  person:
    sources: people-source
    subjects: "http://example.org/$(id)"
    predicateobjects:
      - predicates:
        - value: foaf:name
          targets: predicate-target
        objects:
          value: "$(name)"
      - predicates: foaf:age
        objects:
          value: "$(age)"
